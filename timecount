#include <iostream>
#include <ctime>
using namespace std;
 
int main() {
    time_t start, finish;
    double duration;
 
    start = time(NULL); 
 
    /*실행 시간을 측정하고 싶은 코드*/
 
    finish  = time(NULL); 
    
    duration = (double)(finish - start);
    cout << duration << "초" << endl;
    
    return 0;
}
